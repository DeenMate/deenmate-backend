{
  "name": "deenmate-api",
  "version": "1.0.0",
  "description": "Production backend for DeenMate â€” Islamic content APIs",
  "author": "DeenMate Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:admin": "cd admin-dashboard && npm ci && npm run build",
    "build": "npm run build:admin && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prayer": "ts-node prisma/prayer-seed.ts",
    "db:studio": "prisma studio",
    "enrich:prayer:locations": "node scripts/enrich-prayer-locations.js",
    "backfill:quran:bn": "node scripts/backfill-quran-chapter-bn-names.js",
    "seed:prayer:locations": "ts-node scripts/seed-prayer-locations.ts",
    "global:prayer:sync": "ts-node scripts/global-prayer-sync.ts",
    "seed:prayer:methods": "ts-node scripts/seed-prayer-methods.ts",
    "seed:prayer:cities": "ts-node scripts/seed-prayer-cities.ts",
    "foreground:prayer:prewarm": "ts-node scripts/foreground-prewarm.ts",
    "clear:sync:today": "ts-node scripts/clear-and-sync-today.ts",
    "test:single:prayer": "ts-node scripts/test-single-prayer-sync.ts",
    "simple:sync:today": "ts-node scripts/simple-sync-today.ts",
    "direct:sync:today": "ts-node scripts/direct-sync-today.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.0.0",
    "@prisma/client": "^5.0.0",
    "@types/cheerio": "^0.22.35",
    "@types/luxon": "^3.7.1",
    "adhan": "^4.4.3",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "cheerio": "^1.1.2",
    "decimal.js": "^10.6.0",
    "ioredis": "^5.7.0",
    "luxon": "^3.7.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "retry-axios": "^3.1.3",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "tz-lookup": "^6.1.25"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
